{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["es5"],
      "noImplicitAny": false,
      "noEmitOnError": true,
      "sourceMap": false,
      "strict": true,
      "removeComments": true,
      "downlevelIteration": true,
      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "noUnusedParameters": true
    },
    "exclude": ["node_modules"],
    "include": ["**/*.ts"]
}
  